        <li className=\"list-group-item d-flex \n                    justify-content-between align-items-center\" key={item.id}>\n                        <span className=\"col-1\">\n                          <span className={`iconfont icon-${item.icon}`}></span>\n                        </span>\n                        <span className=\"col-3\">{item.name}</span>\n                        <span className=\"col-2\">{item.type === 1  ? \"+\":\"-\"}{item.amount}元</span>\n                        <span className=\"col-4\">{item.totime}</span>\n                        <span className=\"col-1\">\n                            <a onClick={(event) => {event.preventDefault(); onModifyItem(item)}}>\n                              <span className=\"iconfont icon-bianji\"></span>\n                            </a>\n                        </span>\n                        <span className=\"col-1\">\n                            <a onClick={(event) => {event.preventDefault(); onDelectItem(item)}}>\n                            <span className=\"iconfont icon-shanchu\"></span>\n                            </a>\n                        </span>\n                    </li>\n                ))\n            }\n        </ul>\n    )\n}\n\nPriceList.propTypes = {\n    items: PropTypes.array.isRequired,\n    onModifyItem:PropTypes.func.isRequired,\n    onDelectItem:PropTypes.func.isRequired\n}\n\nexport default PriceList","/Users/suyuhuan/Desktop/JD/pocket-book/src/components/CreateBtn.js",[],"/Users/suyuhuan/Desktop/JD/pocket-book/src/components/TotalNumber.js",[],"/Users/suyuhuan/Desktop/JD/pocket-book/src/components/Tabs.js",["59"],"import React, { useRef } from 'react'\nimport PropTypes from 'prop-types'\n\n\nexport const Tabs = ({activeIndex, onTabChange,children}) => {\n  const node = useRef(null)\n    return (\n      <ul className=\"nav nav-tabs\">\n          { children && children.map((child,index)=>{\n              const activeClassName = (activeIndex === index) ? 'nav-link active' : 'nav-link'\n              return (\n                <li className=\"nav-item\" key={index} refs={node}>\n                   <a\n                     className={activeClassName}\n                     onClick={(event)=>{onTabChange(event, index)}}\n                   >{child}</a>\n              </li>\n              )\n          })\n          }\n       </ul>\n    )\n\n}\nTabs.protoTypes = {\n  activeIndex: PropTypes.number.isRequired,\n  onTabChange: PropTypes.func.isRequired,\n}\nexport const Tab = ({children}) => <>{children}</>","/Users/suyuhuan/Desktop/JD/pocket-book/src/components/MonthPicker.js",["60","61"],"import React,{useState,useRef,useEffect} from 'react'\nimport PropTypes from 'prop-types'\nimport { numRange,padZero} from \"../utils/utility.js\";\nimport \"./MonthPicker.scss\";\n\nconst MonthPicker = ({year,month,onChange})  => {\n    const [selectdYear, setSelectdYear] = useState(year)\n    const [selectdMonth, setSelectdMonth] = useState(month)\n    const [isOpen, setOpen] = useState(false)\n    const monthRange = numRange(12,1)\n    const yearRange = numRange(10,-3).map(number => number + year)\n\n    let node = useRef(null)\n\n    const toggleDropdown = () => {\n      setOpen(!isOpen)\n    }\n\n    const selectYear = (yearNumber) => {\n        setSelectdYear(yearNumber)\n    }\n\n    const selectMonth = (monthNumber) => {\n        setSelectdMonth(monthNumber)\n        onChange(selectdYear, monthNumber)\n        setOpen(false)\n    }\n    const handleClick = (e) => {\n        if (node.current.contains(e.target)) {\n            return;\n        }\n        setOpen(false);\n    }\n    \n    useEffect(()=>{\n        document.addEventListener('click',handleClick,false)\n        return () => {\n            document.removeEventListener('click',handleClick,false)\n        }\n    },[])\n   return (\n       <div className=\"dropdown\" ref={node}>\n           <button\n           type=\"button\"\n           className=\"btn dropdown-toggle text-white\"\n           id =\"dropdownMenu\"\n           onClick={(e)=>{e.preventDefault(); toggleDropdown()}}\n           >{`${year}年 ${padZero(month)}月`}</button>\n\n           {isOpen &&\n           <div className=\"dropdown-menu picker-menu\" aria-labelledby=\"dropdownMenu\" style={{display:'block'}}>\n              <div className=\"d-flex\">\n                <div className=\"border-right years-range\">\n                    { yearRange.map((yearNumber, index) => \n                    <a key={index}\n                        role=\"button\"\n                        onClick={(e) => {e.preventDefault();selectYear(yearNumber)}} \n                        className={(yearNumber === selectdYear) ? \"dropdown-item active text-white\" : \"dropdown-item\"}>\n                        {yearNumber} 年\n                    </a>  \n                    )}\n                </div>\n                <div className=\"months-range\">\n                {monthRange.map((monthNumber, index) => \n                    <a key={index}\n                        role=\"button\"\n                        onClick={(e) => {e.preventDefault();selectMonth(monthNumber)}}\n                        className={(monthNumber === selectdMonth) ? \"dropdown-item active text-white\": \"dropdown-item\"}>\n                        {padZero(monthNumber)} 月\n                    </a>\n                    )}\n                </div>\n               </div>\n           </div>\n           }\n       </div>\n   )\n}\n\n\nMonthPicker.propTypes = {\n    year: PropTypes.number.isRequired,\n    month: PropTypes.number.isRequired,\n    onChange: PropTypes.func.isRequired,\n}\nexport default MonthPicker","/Users/suyuhuan/Desktop/JD/pocket-book/src/dataCenter/testData.js",[],["62","63"],"/Users/suyuhuan/Desktop/JD/pocket-book/src/utils/utility.js",[],"/Users/suyuhuan/Desktop/JD/pocket-book/src/dataCenter/picker.js",[],{"ruleId":"64","replacedBy":"65"},{"ruleId":"66","replacedBy":"67"},{"ruleId":"68","severity":1,"message":"69","line":18,"column":29,"nodeType":"70